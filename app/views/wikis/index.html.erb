<div class="row">
  <div class="col-md-8">
    <h1>All Wikis</h1>
  </div>
  <div class="col-md-4">
    <%= link_to "New Wiki", new_wiki_path, class: 'h1 btn btn-success' %>
  </div>
</div>
<% @wikis.each do |wiki| %>
  <% if wiki_viewable(wiki) %>
  <div class="media">
    <div class="media-body">
      <h4 class="media-heading">
        <p>
          <%= link_to wiki.title, wiki %>
          <% if wiki.private? %>
            &nbsp;(private)
          <% end %>
        </p>
      </h4>
    </div>
  </div>
  <% end %>
<% end %>
